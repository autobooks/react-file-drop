{
  "compilerOptions": {
    "outDir": "../../dist/FileDrop",
    "baseUrl": "../../",
    "module": "commonjs",
    "target": "es5",
    "moduleResolution": "node",
    "jsx": "react",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "traceResolution": false,
    "declaration": true
  },
  "exclude": ["node_modules"]
}
